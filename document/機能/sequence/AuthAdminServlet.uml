@startuml
actor User
participant AuthAdminServlet as A
participant userDao as B
participant loginInfoDto as C
participant users as D
activate A
A -> B: login(loginId, password)
activate B
B -> D: SELECT()
activate D
D -> B: 戻り値
B -> C: 生成
activate C
B -> C: setId(Id)
B -> C: setUserName(userName)
B -> C: setIsAdmin(true)
@enduml